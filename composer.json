{
    "name": "shso/laravel-cassandra",
    "type": "library",
    "description": "Cassandra based query builder for laravel.",
    "license": "MIT",
    "keywords": [
        "cassandra",
        "nosql",
        "laravel",
        "database",
        "query builder",
        "cql"
    ],
    "require": {
        "php": "^5.6 || ^7.0 || ^7.1",
        "ext-cassandra": "^1.3"
    },
    "require-dev": {
        "datastax/php-driver": "^1.3",
        "phpunit/phpunit": "^7.2",
        "johnkary/phpunit-speedtrap": "^3.0",
        "orchestra/testbench": "~3.0"
    },
    "authors": [
        {
            "name": "Shahin Sorkh",
            "email": "sorkh.shahin@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ShSo\\Lacassa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShSo\\Lacassa\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShSo\\Lacassa\\CassandraServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [ "ShSo\\Lacassa\\CassandraServiceProvider" ]
        }
    }
}
